#: src/main.c:17
#, c-format
msgid ""
"Supported units: C, F, K\n"
"\n"
msgstr "Поддерживаемые единицы: C, F, K\n\n"

#: src/main.c:18
#, c-format
msgid "Please provide all arguments in the correct order.\n"
msgstr "Пожалуйста, укажите все аргументы в правильном порядке.\n"

#: src/main.c:29
#, c-format
msgid "Error: invalid unit or conversion\n"
msgstr "Ошибка: неправильная единица или невозможная конверсия\n"

#: src/main.c:30
#, c-format
msgid "Make sure <from_unit> and <to_unit> are one of: C, F, K.\n"
msgstr "Убедитесь, что <исходная_единица> и <целевая_единица> — C, F или K.\n"


